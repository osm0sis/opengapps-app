    <!--______APP INTRO______-->
        <!--TITLES-->
            <string name="appintro_introslide_title">Καλωσήρθατε στην επίσημη εφαρμογή του OpenGApps.org</string>
            <string name="appintro_storageperm_title">Άδεια χρήσης χώρου αποθήκευσης</string>
            <string name="appintro_root_title">Άδεια χρήσης Root</string>
            <string name="appintro_label_architecture">Αρχιτεκτονική CPU</string>
