            <string name="appintro_introslide_title">Selamat datang di Official OpenGApps.org App</string>
            <string name="appintro_storageperm_title">Izin Berkas Penyimpanan</string>
            <string name="appintro_root_title">Izin Akses Root</string>
            <string name="appintro_label_architecture">Arsitektur CPU</string>
