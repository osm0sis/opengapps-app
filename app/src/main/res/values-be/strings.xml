        <!--TITLES-->
            <string name="appintro_introslide_title">Сардэчна запрашаем у афіцыйнае прыкладанне OpenGApps.org</string>
            <string name="appintro_storageperm_title">Доступ да файлаў</string>
            <string name="appintro_root_title">Правы адміністратара</string>
