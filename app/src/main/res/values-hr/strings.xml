        <!--TITLES-->
            <string name="appintro_introslide_title">Dobro došli u službenu OpenGApps.org aplikaciju!</string>
            <string name="appintro_storageperm_title">Dopuštenje za pristup pohrani</string>
            <string name="appintro_root_title">Root dopuštenje</string>
            <string name="appintro_label_architecture">Arhitektura CPU-a</string>
