            <string name="appintro_introslide_title">Welkom bij de officiële OpenGApps.org-app</string>
            <string name="appintro_storageperm_title">Bestandssysteem-rechten</string>
            <string name="appintro_root_title">Root-rechten</string>
            <string name="appintro_label_android">Android-versie</string>
