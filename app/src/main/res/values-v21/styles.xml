<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="SpecialNavD" parent="AppTheme">
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#1A00796B</item>
    </style>

    <style name="intro_radio_button" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:textStyle">normal|bold</item>
        <item name="android:textColor">#ffff</item>
        <item name="android:buttonTint">#ffff</item>
    </style>
</resources>
